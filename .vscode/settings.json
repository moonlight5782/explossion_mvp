{
  "gitdoc.enabled": true,
  "gitdoc.autoPush": true,
  "gitdoc.commitMessage": "Auto commit from VS Code",
  "gitdoc.ignored": [
    "node_modules/**",
    ".next/**",
    ".git/**",
    "out/**",
    "dist/**"
  ],
  "gitdoc.fileFilter": [
    "**/*.js",
    "**/*.ts",
    "**/*.jsx",
    "**/*.tsx",
    "**/*.css",
    "**/*.html",
    "**/*.json"
  ]
}
